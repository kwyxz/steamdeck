{
	"blend": {
		"equation": "add",
		"srcColor": "1",
		"dstColor": "0",
		"srcAlpha": "1",
		"dstAlpha": "0"
	},
	"depth": {
		"function": "always",
		"writeenable": false
	},
	"cull": {
		"mode": "none"
	},
	"write": {
		"rgb": true,
		"alpha": true
	},
	"vertex": "chains/eagle/vs_eagle",
