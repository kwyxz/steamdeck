	"uniforms": [
		{ "name": "decal",           "type": "int",  "values": [ 1.0 ] },
		{ "name": "LUT",             "type": "int",  "values": [ 1.0 ] },
		{ "name": "u_tex_size0",     "type": "vec4", "values": [ 1.0, 1.0, 0.0, 0.0 ] },
		{ "name": "u_inv_view_dims", "type": "vec4", "values": [ 1.0, 1.0, 0.0, 0.0 ] }
	]
}
