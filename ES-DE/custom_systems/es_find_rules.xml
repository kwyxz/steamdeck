<?xml version="1.0"?>
<ruleList>
    <emulator name="BIGPEMUKW">
        <!-- Jaguar Emulator BigPEmu -->
        <rule type="staticpath">
            <entry>~/Applications/BigPEmu/bigpemu.sh</entry>
        </rule>
    <emulator name="CITRAKW">
        <!-- Nintendo 3DS emulator Citra -->
        <rule type="staticpath">
            <entry>~/Applications/citra/citra.AppImage</entry>
        </rule>
    </emulator>
    <emulator name="MAMEKW">
        <!-- MAME arcade emulator -->
        <rule type="staticpath">
            <entry>/var/lib/flatpak/exports/bin/org.mamedev.MAME</entry>
        </rule>
    </emulator>
</ruleList>
